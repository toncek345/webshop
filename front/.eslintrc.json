{
    "extends": "airbnb",
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
  ],
  "rules": {
      "max-len": 0,
      "react/jsx-filename-extension": 0,
      "no-unused-vars": 0,
      "react/prop-types": 0,
      "import/prefer-default-export": 0,
      "no-param-reassign": 0,
      "no-shadow": 0,
      "no-undef": 1,
      "class-methods-use-this": 0,
      "jsx-a11y/anchor-is-valid": [
        "error", {
          "components": [ "Link" ],
          "specialLink": [ "to", "hrefLeft", "hrefRight" ],
          "aspects": [ "noHref", "invalidHref", "preferButton" ]
        }
      ]
  }
}
